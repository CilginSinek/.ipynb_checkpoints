    "request = [[1,'a',['cat'],2],[[[3]],'dog'],4,5]\n",
    "\n",
    "flattedList=[]\n",
    "\n",
    "def FlatingFonks(n):\n",
    "    for index in n:\n",
    "        if isinstance(index,list):\n",
    "            FlatingFonks(index)\n",
    "        else:\n",
    "            flattedList.append(index)\n",
    "\n",
    "FlatingFonks(request)\n",
    "\n",
    "print(flattedList)"
